			"width": "100%",
			"height": "100%"
		},
		"children": [
			{
				"type": "element",
				"name": "separator",
				"layout": {
					"size": [ 5, 5 ],
					"width_alloc": "px",
					"height_alloc": "*",
					"margin": 0.0,
					"anchor": "ltrb"
				},
				"cursor": "arrow_east_west",
				"visuals": {
					"geometries": [
						{
							"type": "path",
							"subpaths": [
								[
									[
										[ 0.5, 0.0 ],
										[ 0.0, 2.0 ]
									],
									{
										"line_to": [
											[ 0.5, 1.0 ],
											[ 0.0, -2.0 ]
										]
									},
									{ "closed": false }
								]
							],
							"stroke": {
								"type": "solid",
								"color": [ 0.8, 0.8, 0.8, 0.0 ],
								"thickness": 1
							}
						}
					]
				},
				"animations": {
					"main.set_horizontal": {
						"layout.width_alloc": "px",
						"layout.height_alloc": "*",
						"cursor": "arrow_east_west",
						"visuals.geometries[0].(path.subpaths)[0].starting_point": {
							"to": [
								[ 0.5, 0.0 ],
								[ 0.0, 2.0 ]
							]
						},
						"visuals.geometries[0].(path.subpaths)[0].parts[0].(segment.to)": {
							"to": [
								[ 0.5, 1.0 ],
								[ 0.0, -2.0 ]
							]
						}
					},
					"main.set_vertical": {
						"layout.width_alloc": "*",
						"layout.height_alloc": "px",
						"cursor": "arrow_north_south",
						"visuals.geometries[0].(path.subpaths)[0].starting_point": {
							"to": [
								[ 0.0, 0.5 ],
								[ 2.0, 0.0 ]
							]
						},
						"visuals.geometries[0].(path.subpaths)[0].parts[0].(segment.to)": {
							"to": [
								[ 1.0, 0.5 ],
								[ -2.0, 0.0 ]
							]
						}
					},

					"mouse_enter": {
						"visuals.geometries[0].stroke.(solid_color_brush.color).a": {
							"to": 1.0,
							"duration": 0.1,
							"transition": "linear"
						}
					},
					"mouse_leave": {
						"visuals.geometries[0].stroke.(solid_color_brush.color).a": {
							"to": 0.0,
							"duration": 0.1,
							"transition": "linear"
						}
					}
				}
			}
		]
	},

	"code_editor": {
		"name": "main",
		"layout": {
			"width": "100%",
			"height": "100%"
		},
		"children": [
			{
				"type": "scrollbar",
				"name": "vertical_scrollbar",
				"class": "scrollbar",
				"extras": {
					"orientation": "vertical"
				},
				"layout": {
					"anchor": "tbr",
					"width": "8px",
					"height": "100%",
					"margin": [ 0.0, 0.0, 0.0, 8.0 ]
				}
			},
			{
				"type": "scrollbar",
				"name": "horizontal_scrollbar",
				"class": "scrollbar",
				"extras": {
					"orientation": "horizontal"
				},
				"layout": {
					"anchor": "lbr",
					"width": "100%",
					"height": "8px",
					"margin": [ 0.0, 0.0, 8.0, 0.0 ]
				}
			},
			{
				"type": "stack_panel",
				"name": "components_panel",
				"visibility": "lvi",
				"extras": {
					"orientation": "horizontal"
				},
				"layout": {
					"width": "100%",
					"height": "100%",
					"margin": [ 0.0, 0.0, 8.0, 8.0 ]
				},
				"children": [
					{
						"type": "line_number_display",
						"layout": {
							"padding": [ 10, 0, 5, 0 ]
						}
					},
					{
						"type": "code_contents_region",
						"name": "contents_region",
						"visibility": "lvi",
						"extras": {
							"selection_brush": {
								"type": "solid",
								"color": [ 0.2, 0.5, 1.0, 0.2 ]
							},
							"selection_pen": {
								"type": "solid",
								"color": [ 0.0, 0.0, 0.0, 1.0 ]
							},
							"caret_visuals": {
								"geometries": [
									{
										"type": "rectangle",
										"top_left": [
											[ 0.0, 0.0 ],
											[ 0.0, 0.0 ]
										],
										"bottom_right": [
											[ 0.0, 1.0 ],
											[ 2.0, 0.0 ]
										],
										"fill": [ 1.0, 1.0, 1.0, 1.0 ]
									}
								]
							}
						},
						"layout": {
							"height": "100%",
							"padding": [ 2.0, 0.0, 0.0, 0.0 ]
						},
						"animations": {
							"carets_changed": {
								"caret_visuals.geometries[0].fill.(solid_color_brush.color).a": {
									"frames": [
										{
											"to": 1.0,
											"duration": 0.0
										},
										{
											"to": 1.0,
											"duration": 0.3
										},
										{
											"to": 0.8,
											"duration": 0.05
										},
										{
											"to": 0.6,
											"duration": 0.05
										},
										{
											"to": 0.4,
											"duration": 0.05
										},
										{
											"to": 0.2,
											"duration": 0.05
										},
										{
											"to": 0.1,
											"duration": 0.05
										},
										{
											"to": 0.0,
											"duration": 0.05
										},
										{
											"to": 1.0,
											"duration": 0.4
										}
									],
									"repeat": 10
								}
							},

							"mode_changed_insert": {
								"caret_visuals.geometries[0].(rectangle.bottom_right)": {
									"to": [
										[ 0.0, 1.0 ],
										[ 2.0, 0.0 ]
									],
									"duration": 0.1,
									"transition": "convex_quadratic"
								}
							},
							"mode_changed_overwrite": {
								"caret_visuals.geometries[0].(rectangle.bottom_right)": {
									"to": [
										[ 1.0, 1.0 ],
										[ 0.0, 0.0 ]
									],
									"duration": 0.1,
									"transition": "convex_quadratic"
								}
							},

							"main.lost_focus": {
								"selection_brush.(solid_color_brush.color)": {
									"to": [ 1.0, 1.0, 1.0, 0.2 ],
									"duration": 0.1,
									"transition": "linear"
								}
							},
							"main.got_focus": {
								"selection_brush.(solid_color_brush.color)": {
									"to": [ 0.2, 0.5, 1.0, 0.2 ],
									"duration": 0.1,
									"transition": "linear"
								}
							}
						}
					},
					{
						"type": "minimap",
						"extras": {
							"viewport_visuals": {
								"geometries": [
									{
										"type": "rectangle",
										"top_left": [
											[ 0.0, 0.0 ],
											[ 0.0, 0.0 ]
										],
										"bottom_right": [
											[ 1.0, 1.0 ],
											[ 0.0, 0.0 ]
										],
										"fill": {
											"type": "solid",
											"color": [ 1.0, 1.0, 1.0, 0.1 ]
										}
									}
								]
							}
						},
						"animations": {
							"mouse_enter": {
								"viewport_visuals.geometries[0].fill.(solid_color_brush.color).a": {
									"to": 0.3,
									"duration": 0.1,
									"transition": "linear"
								}
							},
							"mouse_leave": {
								"viewport_visuals.geometries[0].fill.(solid_color_brush.color).a": {
									"to": 0.1,
									"duration": 0.1,
									"transition": "linear"
								}
							}
						},
						"layout": {
							"height": "100%"
						}
					}
				]
			}
		]
	},
