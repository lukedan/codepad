{
	"graphics_backend": "direct2d",
	"native_plugins": [
		"editors",
		"command_pack",
		"python_plugin_host_pybind11",
		"tree_sitter",
		"lsp"
	],
	"editor": {
		"font_size": 12,
		"font_family": "Cascadia Code",
		"backup_fonts": [
			"SimSun",
			"MingLiu-ExtB",
			"Segoe UI Emoji",
			"Arial"
		],
		"interaction_modes": [
			"prepare_drag",
			"single_selection"
		],

		"theme": { // https://github.com/tomasiser/vim-code-dark
			"keyword": {
				"color": "#569CD6",
				"weight": "bold"
			},
			"operator": [ 0.863, 0.863, 0.667, 1.0 ],
			"string": "#CE9178",
			"comment": "#608B4E",
			"type": "#4EC9B0",
			"number": "#B5CEA8",
			"tag": [ 0.337, 0.612, 0.839, 1.0 ],

			"class": "#4EC9B0",
			"enum": "#4EC9B0",
			"macro": "#C586C0",
			"variable": "#9CDCFE"
		}
	}
}
